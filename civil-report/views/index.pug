extends layout

block link
  -var selected = 'Home';

block content
    .container
        if (!user)
            a(href="/login") Login
            br
            a(href="/register") Register
        if (user)
            p You are currently logged in as Civil Servant
            a(href="/logout") Logout

    h1= title
    p Welcome to the #{title}
        .row
            .col-md-12
                .page-header
                    h1 Please right click the street where the problem is located
    div#map
    include modal.pug
